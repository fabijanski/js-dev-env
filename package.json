{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "localtunnel-other-domain": "lt --port 3000 --subdomain fbiproject",
    "share": "npm-run-all --parallel open:src localtunnel",
    "share-other-domain": "npm-run-all --parallel open:src localtunnel-other-domain",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001"
  },
  "author": "fabijanski",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-register": "6.26.0",
    "browser-sync-webpack-plugin": "^1.2.0",
    "chai": "4.1.2",
    "chalk": "2.1.0",
    "cheerio": "1.0.0-rc.2",
    "compression": "1.7.0",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "eslint": "4.5.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-watch": "3.1.2",
    "express": "4.15.4",
    "extract-text-webpack-plugin": "3.0.0",
    "html-webpack-plugin": "2.30.1",
    "jsdom": "11.2.0",
    "json-schema-faker": "0.4.3",
    "json-server": "0.12.0",
    "localtunnel": "1.8.3",
    "mocha": "3.5.0",
    "nock": "9.0.14",
    "node-sass": "^4.5.3",
    "npm-run-all": "4.1.1",
    "nsp": "2.7.0",
    "numeral": "2.0.6",
    "open": "0.0.5",
    "postcss-loader": "^2.0.6",
    "rimraf": "2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "0.18.2",
    "webpack": "3.5.5",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.18.2",
    "webpack-md5-hash": "0.0.5"
  }
}
